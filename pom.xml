<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>io.github.bananapuncher714</groupId>
	<artifactId>cartographer2_master</artifactId>
	<version>2.14.7</version>
	<packaging>pom</packaging>

	<properties>
		<master.version>2.14.7</master.version>
	</properties>

	<modules>
		<module>cartographer2_main</module>
		<module>cartographer2_v1_8_r3</module>
		<module>cartographer2_v1_9_r2</module>
		<module>cartographer2_v1_10_r1</module>
		<module>cartographer2_v1_11_r1</module>
		<module>cartographer2_v1_12_r1</module>
		<module>cartographer2_v1_13_r2</module>
		<module>cartographer2_v1_14_r1</module>
		<module>cartographer2_v1_15_r1</module>
		<module>cartographer2_v1_16_r1</module>
		<module>cartographer2_v1_16_r2</module>
		<module>cartographer2_v1_16_r3</module>
		<module>cartographer2</module>
		<module>cartographer2_api</module>
		<module>cartographer2_modulevanilla</module>
		<module>cartographer2_moduleexperimental</module>
		<module>cartographer2_moduleworldviewer</module>
		<module>cartographer2_modulefactionsuuid</module>
		<module>cartographer2_moduletowny</module>
		<module>cartographer2_moduleresidence</module>
		<module>cartographer2_moduleguilds</module>
		<module>cartographer2_moduleworldguard_master</module>
		<module>cartographer2_modulelands</module>
	</modules>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<distributionManagement>
		<repository>
			<id>github</id>
			<name>GitHub BananaPuncher714 Apache Maven Packages</name>
			<url>https://maven.pkg.github.com/BananaPuncher714/Cartographer2</url>
		</repository>
	</distributionManagement>
</project>